{
  "publicPath": "/",
  "hash": "contenthash",
  "outputAssetsPath": {
    "js": "static/js",
    "css": "static/css"
  },
  "devServer": {
    "historyApiFallback": true
  },
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": "@alifd/theme-design-pro"
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": ["zh-cn", "zh-tw"]
      }
    ],
    [
      "build-plugin-antd",
      {
        "themeConfig": {
          "primary-color": "#209BFA"
        },
        "importOptions": {
          "libraryDirectory": "lib"
        }
      }
    ],
    "./build.plugin.js"
  ]
}
